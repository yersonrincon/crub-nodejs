    <diV class="container">
    <a href="/create" class=" btn btn-outline-primary mt-4"><i class='bx bx-add-to-queue bx-md'></i></a>
    <form  action="/consultar" method="POST">
      <div class="col-md-3">
        <label  class="form-label">GENERO</label>
        <select id="genero" name="genero" class="form-select" tabindex="2">
          <option value="masculino">MASCULINO</option>
          <option value="femenino">FEMENINO</option>
        </select>
        <button type="submit" class="btn btn-primary" tabindex="5">consultar</button>
      </div>
    </form>
      <table  id ="usuarios"  class="table  table-bordered table-striped text-center mt-4">
        <thead>
            <tr class="bg-primary text-white"></tr>
            <th scope="col">ID</th>
            <th scope="col">NOMBRE</th>
            <th scope="col">APELLIDO</th>
            <th scope="col">DIRECCION</th>
            <th scope="col">EDAD</th>
            <th scope="col">GENERO</th>
            <th scope="col">ACCIONES</th>
        </thead>
        <tbody>
          <% results.forEach((usuarios) =>{ %>
          <tr>
            <td><%= usuarios.id %></td>
            <td><%= usuarios.nombre %></td>
            <td><%= usuarios.apellido %></td>
            <td><%= usuarios.direccion %></td>
            <td><%= usuarios.edad %></td>
            <td><%= usuarios.genero %></td>
            <td>
            <a href="/edit/<%= usuarios.id %>"class="btn btn-info"><i class="fas fa-edit"></i></a><a onclick="confirmar(<%= usuarios.id %>)" class="btn btn-danger"><i class="fas fa-trash-alt"></i></a>
          </tr>
        </td>
          <% }) %>
          
        </tbody>
      </table>
       </diV>
