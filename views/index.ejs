     <form  action="/edad" method="POST"> 
            <div class="col-md-3">
              <label  class="form-label">EDAD</label>
              <select id="edad" name="edad" class="form-select" tabindex="2">
                <option value=">=45">MAYOR A 45</option>
                <option value="<=33"> MENOR A 33</option>
